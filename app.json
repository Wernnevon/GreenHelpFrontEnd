{
    "expo": {
        "name": "greenhelp",
        "slug": "greenhelp",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/green/icon.png",
        "splash": {
            "image": "./assets/green/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#fff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}